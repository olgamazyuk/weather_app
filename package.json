{
  "name": "react-redux-starter-hooks",
  "version": "0.1.0",
  "private": true,
  "author": "Effective Soft Team",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.effective-soft.com/projects/ES617/repos/react-redux-starter-hooks"
  },
  "engines": {
    "node": ">=12.13.0",
    "yarn": ">=1.19.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint",
    "**/*.js": "stylelint --config stylelintrc.json",
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "babelMacros": {
    "styledComponents": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version",
      "last 1 opera version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.19.1",
    "axios-mock-adapter": "^1.18.1",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-xhr-backend": "^3.2.2",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "react-i18next": "^11.5.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-use": "^15.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.1.1",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.0.10",
    "jest": "24.9.0",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "source-map-explorer": "^2.4.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0"
  }
}
